{
  "name": "expression-atlas-tsne-plot",
  "version": "0.1.0",
  "description": "A t-SNE visualisation component for Single Cell Expression Atlas",
  "main": "lib/index.js",
  "scripts": {
    "dist": "NODE_ENV=production webpack --optimize-minimize",
    "prepublishOnly": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Expression Atlas developers <arrayexpress-atlas@ebi.ac.uk>",
  "collaborators": [
    "Elisabet Barrera <barrera@ebi.ac.uk>",
    "Wojciech Bażant <wbazant@ebi.ac.uk>",
    "Alfonso Muñoz-Pomer Fuentes <amunoz@ebi.ac.uk>"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxa/scxa-tsne-plot.git"
  },
  "dependencies": {
    "color": "^2.0.0",
    "highcharts": "^5.0.14",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-highcharts": "^12.0.0",
    "react-refetch": "^1.0.0",
    "recompose": "^0.25.0",
    "urijs": "^1.18.12"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.23.1",
    "enzyme": "^2.9.1",
    "file-loader": "^0.8.4",
    "image-webpack-loader": "^1.6.3",
    "imports-loader": "^0.6.5",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "react-test-renderer": "^15.6.1",
    "request": "^2.75.0",
    "style-loader": "^0.13.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1",
    "whatwg-fetch": "^2.0.3"
  }
